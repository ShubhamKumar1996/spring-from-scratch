<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://www.springframework.org/schema/context
	   http://www.springframework.org/schema/context/spring-context.xsd">

    <context:annotation-config/>

    <bean class="com.example.springcoreproject2_1QualifierInAutowiring.HardDisk" name="hardDisk1">
        <property name="size" value="1"/>
        <property name="speed" value="2"/>
    </bean>
    <bean class="com.example.springcoreproject2_1QualifierInAutowiring.HardDisk" name="hardDisk2">
        <property name="size" value="2"/>
        <property name="speed" value="3"/>
    </bean>

    <bean class="com.example.springcoreproject2_1QualifierInAutowiring.Laptop" name="laptop1">
        <property name="laptopId" value="101"/>
    </bean>

</beans>
